#!/usr/bin/env python

import sys

if len(sys.argv) > 1:
   ccs = [c for c in sys.argv[1]]
else:
   ccs = ["C"]
   
# 32768 alphabet = ["*", "i", "h", "c", "f", "d", "L", "l"]
alphabet = ["p", "i", "h", "l"]

result = []

digits = None
def increaseDigit(v):
  if v is None:
    v = 0
  else:
    v = v + 1
  return(v)
def increase():
    global digits
    i = len(digits) - 1
    digits[i] = increaseDigit(digits[i])
    while i >= 0 and digits[i] >= len(alphabet):
        digits[i] -= len(alphabet)
        i = i - 1
        if i < 0:
            return(False)
        digits[i] = increaseDigit(digits[i])
    return(True)
def reset(cc):
    global digits
    if cc == "P":
        digits = 5 * [None]
    else:
        digits = 5 * [0]
def galphabet(a):
    if a is None:
        return ""
    else:
        return alphabet[a]
extras = [
