#ifndef __4D_FFIS_ARGUMENTMARSHALLER_H
#define __4F_FFIS_ARGUMENTMARSHALLER_H
#include "Evaluators/FFI"
namespace FFIs {

struct ArgumentMarshaller : public Evaluators::FFI {
private:
	AST::Symbol* resultSignature;
public:
	ArgumentMarshaller(AST::Symbol* resultSignature);
	virtual AST::Node* executeLowlevel(AST::Node* argument);
};

};
#endif /*ndef __4D_FFIS_ARGUMENTMARSHALLER_H*/
